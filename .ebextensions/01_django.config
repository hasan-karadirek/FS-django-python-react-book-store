option_settings:

  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: "bookStoreApi.settings"
    DEBUG: "False"

  aws:elasticbeanstalk:environment:process:default:
    DJANGO_SUPERUSER_USERNAME: "admin"  # Replace with your desired superuser username
    DJANGO_SUPERUSER_PASSWORD: "HasAdmin123"  # Replace with your desired superuser password
    DJANGO_SUPERUSER_EMAIL: "hasankaradirek3@gmail.com"  # Replace with your desired superuser email
files:
  "/opt/elasticbeanstalk/hooks/appdeploy/post/99_run_migrations.sh":
    mode: "000755"
    owner: root
    group: root
    content: |
      #!/usr/bin/env bash
      CONTAINER_ID=$(docker ps -q)
      docker exec $CONTAINER_ID python manage.py migrate --noinput



